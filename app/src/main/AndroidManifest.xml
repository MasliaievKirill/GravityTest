<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.masliaiev.gravitytest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name=".presentation.GravityTestApp"
        android:icon="@mipmap/ic_launcher_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GravityTest">
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>